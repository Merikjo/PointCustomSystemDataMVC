<h2>Hoitaja</h2>

<p style="padding-left:8px;font-size:12px">
    @Html.ActionLink("Luo uusi hoitajatieto", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User.UserIdentity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Identity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone1.PhoneNum_1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PostOffices1.PostalCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PostOffices1.PostOffice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Notes)
        </th>    
        <th>
            @Html.DisplayNameFor(model => model.EnrollmentDateIN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnrollmentDateOUT)
        </th>   
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User.UserIdentity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Identity)
        </td>    
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone1.PhoneNum_1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostOffices1.PostalCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PostOffices1.PostOffice)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnrollmentDateIN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnrollmentDateOUT)
        </td>
     
      
     
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Student_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Student_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Student_id })
        </td>
    </tr>
}

</table>
